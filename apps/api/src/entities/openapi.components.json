{
  "schemas": {
    "Error": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        }
      }
    },
    "User": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string",
          "format": "email"
        },
        "displayName": {
          "type": "string"
        },
        "id": {
          "type": "integer"
        }
      }
    },
    "Service": {
      "type": "object",
      "properties": {
        "id": {
          "type": "int"
        },
        "type": {
          "type": "string",
          "enum": [
            "google",
            "spotify",
            "apple",
            "deezer",
            "lastfm"
          ]
        },
        "enabled": {
          "type": "boolean"
        }
      }
    }
  }
}
